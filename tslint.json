{
  "extends": "tslint:latest",
  // "rulesDirectory": ["path/to/custom/rules/directory/", "another/path/"],
  "rules": {
    /*"max-line-length": {
      "options": [120]
    },*/
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "indent": [
      true,
      "tabs"
    ],
    "no-trailing-whitespace": false,
    "trailing-comma": false
  }
  /*"jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }*/
}
